# 应用配置
VITE_APP_NAME=AI Document Editor
VITE_APP_VERSION=0.1.0

# API 配置
VITE_GRAPHQL_URL=/graphql
VITE_API_URL=/api
VITE_WS_URL=ws://localhost:3001/graphql

# AI 配置
VITE_AI_ENABLED=true
VITE_AI_PROVIDER=openai
VITE_AI_API_KEY=your-api-key-here
VITE_AI_MODEL=gpt-4

# 存储配置
VITE_CLOUD_STORAGE=true
VITE_UPLOAD_ENDPOINT=/api/upload

# 功能开关
VITE_ENABLE_EDGELESS=false
VITE_ENABLE_DATABASE=false
VITE_ENABLE_SHARING=false
